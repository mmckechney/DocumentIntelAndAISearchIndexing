  "contentVersion": "1.0.0.0",
  "parameters": {
    "apiManagementPublisherEmail": {
      "value": "${AZURE_CURRENT_USER_EMAIL}"
    },
    "apiManagementPublisherName": {
      "value": "${AZURE_CURRENT_USER_NAME}"
    },
    "appName": {
      "value": "${APP_NAME}"
    },
    "azureOpenAIChatModel": {
      "value": "gpt-4o"
