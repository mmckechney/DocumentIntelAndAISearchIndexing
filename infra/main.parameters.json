{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "${APP_NAME}"
    },
    "appNameSafe": {
      "value": "${APP_NAME_SAFE}"
    },
    "currentUserObjectId": {
      "value": "${AZURE_CURRENT_USER_OBJECT_ID}"
    },
    "docIntelligenceInstanceCount": {
      "value": 2
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "myPublicIp": {
      "value": "${PUBLIC_IP}"
    },
    "foundryConfig": {
      "value": {
        "projectDisplayName": "${APP_NAME} Document Intelligence",
        "embeddingMaxTokens": 8192,
        "chatModel": {
          "name": "gpt-5-mini",
          "version": "2025-08-07",
          "sku": "GlobalStandard",
          "capacity": 100
        },
        "embeddingModel": {
          "name": "text-embedding-3-large",
          "version": "1",
          "sku": "GlobalStandard",
          "capacity": 100
        }
      }
    },
    "foundryAgentId": {
      "value": "${AZURE_FOUNDRY_AGENT_ID}"
    }
  }
}
