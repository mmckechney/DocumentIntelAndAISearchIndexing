{
  "version": "2.0",
   "logging": {
      "logLevel": {
         "default": "Warning",
         "Function": "Debug"
      },
      "applicationInsights": {
         "samplingSettings": {
            "isEnabled": true,
            "excludedTypes": "Request"
         },
         "enableLiveMetricsFilters": true
      }
   },
  "extensions": {
    "blobs": {}
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}