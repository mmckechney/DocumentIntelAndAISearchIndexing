{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
   "serviceBus": {
      "clientRetryOptions": {
         "mode": "exponential",
         "tryTimeout": "00:01:00",
         "delay": "00:00:00.80",
         "maxDelay": "00:01:00",
         "maxRetries": 3
      },
      "prefetchCount": 0,
      "transportType": "amqpWebSockets",
      "maxAutoLockRenewalDuration": "00:10:00",
      "maxConcurrentCalls": 32,
      "autoCompleteMessages": true

   }
  }
}